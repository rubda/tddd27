{
  "name": "express-server",
  "version": "1.0.0",
  "description": "Server for TDDD27",
  "private": true,
  "scripts": {
    "start": "nodemon -L src/server.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "node dist/server.js",
    "prod": "npm run build && npm run serve",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {
    "express": "^4.15.2",
    "body-parser": "^1.17.1",
    "mongoose": "^4.9.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0"
  },
  "author": "Ruben Das"
}
